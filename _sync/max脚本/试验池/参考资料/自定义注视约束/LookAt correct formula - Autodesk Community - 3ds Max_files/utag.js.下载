function trackCcpaEvent(event,ancillaryData){var eventType=false,fireEvent=false,eventData={'category':false,'action':false,'label':false},ancData=ancillaryData||false;switch(event){case'ccpa-link-injected':eventData.category='Privacy Management';eventData.action='CCPA Link Injection';eventData.label=ancData;eventType='event';break;case'ccpa-form-submitted':eventData.category='Privacy Management';if(ancillaryData){eventData.action='CCPA Form Submit Attempt';eventData.label=ancData;}else{eventData.action='CCPA Form Submit Attempt';eventData.label='success';}
eventType='event';break;case'ccpa-error-tracking':eventData.category='Privacy Management';eventData.action='CCPA Error Tracking';eventData.label=ancData;eventType='event';break;case'ccpa-cookie-tracking':eventData.category='Privacy Management';eventData.action='CCPA Cookie Tracking';eventData.label=ancData;eventType='event';break;default:eventData.category='Privacy Management';eventData.action='CCPA Default Event';eventData.label=ancData;eventType='event';break;}
