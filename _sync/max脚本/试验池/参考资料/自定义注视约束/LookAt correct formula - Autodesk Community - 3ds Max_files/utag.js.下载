function buildCleanUrl(which){var cUrl=document.location.hostname+(document.location.pathname.split('\?'))[0],cRef=document.referrer,cPath=(document.location.pathname.split('\?'))[0];var pathChecks=['/user-access/users/user-details/','user-access/users/','products-services/industry-collection/','orders/contracts/','orders/subscriptions/','/users/','/user/','/profile/','/emailaddress','/emailAddress'],allowList=['user-access/users/user-list'],urls=[cUrl,cRef,cPath];for(var y=0;y<urls.length;y+=1){for(var x=0;x<pathChecks.length;x+=1){if(urls[y].indexOf(pathChecks[x])>-1&&urls[y].indexOf(allowList[x])<0){var splitO=urls[y].split(pathChecks[x]);urls[y]=splitO[0]+pathChecks[x]+'-masked-';break;}}}
