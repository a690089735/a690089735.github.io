function cleanUrlOfUniqueness(originalUrl){try{var newUrl=originalUrl;if(strChk(newUrl,'/account/resetpassword')){newUrl=newUrl.replace(new RegExp("/account/resetpassword.*","gm"),"/account/resetpassword");}
