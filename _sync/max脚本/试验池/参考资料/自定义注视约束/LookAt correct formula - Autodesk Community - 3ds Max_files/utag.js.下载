function deleteCrossDomainCookies(){if(typeof window.postWadsCommand==="function"){var wadsCmd='delete-cookies';window.postWadsCommand(wadsCmd);}}}catch(e){utag.DB(e)}}catch(e){}};if(!utag_condload){try{try{var debug=document.location.search.indexOf('wafDebugCCPA')>-1||(window.localStorage&&!!window.localStorage.getItem('wafDebugCCPA'))||(window.sessionStorage&&!!window.sessionStorage.getItem('wafDebugCCPA'));function S4(){return(((1+Math.random())*0x10000)|0).toString(16).substring(1);}
function uuidv4(){try{return([1e7]+-1e3+-4e3+-8e3+-1e11).replace(/[018]/g,function(e){return(e^crypto.getRandomValues(new Uint8Array(1))[0]&15>>e/4).toString(16)})}catch(e){return!1}}
function ccpaGuid(){return uuidv4()||(S4()+S4()+"-"+S4()+"-"+S4()+"-"+S4()+"-"+S4()+S4()+S4());}
function checkCookie(n){for(var t=n+"=",e=document.cookie.split(";"),r=0;r<e.length;r++){for(var i=e[r];" "===i.charAt(0);)i=i.substring(1,i.length);if(0===i.indexOf(t))return i.substring(t.length,i.length)}return!1}
